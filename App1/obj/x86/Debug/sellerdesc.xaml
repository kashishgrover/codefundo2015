<Page
    x:Class="App1.sellerdesc"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:App1"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <TextBlock x:ConnectionId='1' x:Name="Titleop" HorizontalAlignment="Left" Height="29" Margin="10,13,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="56">
        	<Run Text="Title"/>
        	<LineBreak/>
        	<Run/>
        </TextBlock>
        <TextBox x:ConnectionId='2' x:Name="titleop" HorizontalAlignment="Left" Height="0" Margin="71,10,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="279"/>
        <TextBlock x:ConnectionId='3' x:Name="detailsop" HorizontalAlignment="Left" Height="29" Margin="10,42,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="56" Text="Details"/>
        <TextBox x:ConnectionId='4' x:Name="pricedisp" HorizontalAlignment="Left" Height="0" Margin="136,378,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="279" IsReadOnly="True"/>
        <RadioButton x:ConnectionId='5' x:Name="timespecop" Content="Time Specific" HorizontalAlignment="Left" Height="34" Margin="10,133,0,0" VerticalAlignment="Top" Width="146" />
        <RadioButton x:ConnectionId='6' x:Name="dlspecop" Content="Deadline" HorizontalAlignment="Left" Height="34" Margin="10,267,0,0" VerticalAlignment="Top" Width="146" />
        <TextBlock x:ConnectionId='7' x:Name="priceop" HorizontalAlignment="Left" Margin="10,378,0,0" TextWrapping="Wrap" Text="Price" VerticalAlignment="Top" Height="32" Width="121"/>
        <TextBlock x:ConnectionId='8' x:Name="categoryop" HorizontalAlignment="Left" Margin="10,436,0,0" TextWrapping="Wrap" VerticalAlignment="Top" RenderTransformOrigin="-0.355,1.983" >
        	<Run Text="Category"/>
        	<LineBreak/>
        	<Run/>
        </TextBlock>
        <TextBlock x:ConnectionId='9' x:Name="textBlock" HorizontalAlignment="Left" Margin="15,490,0,0" TextWrapping="Wrap" Text="Location" VerticalAlignment="Top"/>
        <TextBox x:ConnectionId='10' x:Name="locationBox" HorizontalAlignment="Left" Margin="108,487,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="242"/>
        <Button x:ConnectionId='11' x:Name="button" Content="Accept task!" HorizontalAlignment="Left" Margin="136,562,0,0" VerticalAlignment="Top" Height="33" Width="174"                     />
        <TextBox x:ConnectionId='12' x:Name="det" HorizontalAlignment="Left" Height="92" Margin="71,47,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="279"/>
        <TextBox x:ConnectionId='13' x:Name="catselop" HorizontalAlignment="Left" Margin="108,436,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="242"/>
        <TextBox x:ConnectionId='14' x:Name="datedead" HorizontalAlignment="Left" Height="0" Margin="136,333,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="279" IsReadOnly="True"/>
        <TextBox x:ConnectionId='15' x:Name="timedead" HorizontalAlignment="Left" Height="0" Margin="136,301,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="279" IsReadOnly="True"/>
        <TextBox x:ConnectionId='16' x:Name="timestart" HorizontalAlignment="Left" Height="0" Margin="136,167,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="279" IsReadOnly="True"/>
        <TextBox x:ConnectionId='17' x:Name="timeend" HorizontalAlignment="Left" Height="0" Margin="136,199,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="279" IsReadOnly="True"/>
        <TextBox x:ConnectionId='18' x:Name="datespec" HorizontalAlignment="Left" Height="0" Margin="136,231,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="279" IsReadOnly="True"/>
        <TextBlock x:ConnectionId='19' x:Name="textBlock1" HorizontalAlignment="Left" Height="32" Margin="15,167,0,0" TextWrapping="Wrap" Text="Start Time" VerticalAlignment="Top" Width="121"/>
        <TextBlock x:ConnectionId='20' x:Name="textBlock1_Copy" HorizontalAlignment="Left" Height="32" Margin="15,333,0,0" TextWrapping="Wrap" Text="Date" VerticalAlignment="Top" Width="121"/>
        <TextBlock x:ConnectionId='21' x:Name="textBlock1_Copy1" HorizontalAlignment="Left" Height="32" Margin="15,301,0,0" TextWrapping="Wrap" Text="Time" VerticalAlignment="Top" Width="121"/>
        <TextBlock x:ConnectionId='22' x:Name="textBlock1_Copy2" HorizontalAlignment="Left" Height="32" Margin="15,231,0,0" TextWrapping="Wrap" Text="Date" VerticalAlignment="Top" Width="121"/>
        <TextBlock x:ConnectionId='23' x:Name="textBlock1_Copy3" HorizontalAlignment="Left" Height="32" Margin="15,199,0,0" TextWrapping="Wrap" Text="End Time" VerticalAlignment="Top" Width="121"/>

    </Grid>
</Page>

